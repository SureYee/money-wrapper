{
    "name": "cleaniquecoders/money-wrapper",
    "description": "Built with Laravel Standalone Package Creator",
    "license": "MIT",
    "authors": [
        {
            "name": "Nasrul Hazim",
            "email": "nasrulhazim.m@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "CleaniqueCoders\\MoneyWrapper\\": "src/"
        },
        "files": [
            "src/Support/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "CleaniqueCoders\\MoneyWrapper\\Tests\\": "tests/"
        }
    },
    "require": {
        "illuminate/support": "^5.5",
        "moneyphp/money": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.5",
        "orchestra/testbench": "~3.0",
        "codedungeon/phpunit-result-printer": "^0.4.4"
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
